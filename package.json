{
  "name": "my-vue-scaffold",
  "version": "1.0.0",
  "description": "this is a vue scaffold",
  "main": "index.js",
  "scripts": {
    "local": "npm run dev --config=local",
    "dev": "webpack-dev-server -d --hot --inline --env.dev --env.config dev --host 0.0.0.0",
    "build": "rimraf dist && webpack -p --env.config prod"
  },
  "author": "xiaobin_wu",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-enough": "^0.2.6",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^2.0.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^2.28.0",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.5",
    "rimraf": "^2.6.1",
    "sass": "^0.5.0",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.8",
    "vue-loader": "^12.2.1",
    "vue-skeleton-webpack-plugin": "^1.1.5",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.4.5",
    "webpack-node-externals": "^1.6.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "enough",
    "env": {
      "browser": true,
      "node": true,
      "commonjs": true,
      "es6": true
    },
    "rules": {
      "linebreak-style": 0,
      "indent": [
        2,
        4
      ],
      "no-unused-vars": 0,
      "no-console": 0
    },
    "plugins": [
      "html"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ],
      "stage-2"
    ]
  },
  "dependencies": {
    "vue": "^2.3.3",
    "vue-router": "^2.5.3",
    "vuex": "^2.3.1"
  }
}
